{
  "name": "rxjs-inspector",
  "version": "0.1.0",
  "type": "commonjs",
  "description": "Instrumentation, logging, and visualization tools for RxJS operator chains.",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "rxjs-inspector": "dist/cli/bin.cjs",
    "rxjs-inspector-server": "dist/web/server.cjs"
  },
  "scripts": {
    "simple": "ts-node src/simple.ts",
  "inspect:summary": "ts-node src/cli/summarize-log.ts",
  "inspect:timeline": "ts-node src/cli/print-log.ts",
  "inspect:mermaid": "ts-node src/cli/to-mermaid.ts",
    "build": "tsup",
    "test": "vitest",
    "dev:web": "tsx src/web/server.ts",
    "dev:cli": "tsx src/cli/bin.ts",
    "dashboard": "tsx src/web/dashboard.ts",
    "demo:simple": "tsx src/example/simple.ts",
    "demo:advanced": "tsx src/example/advanced-demo.ts",
    "demo:custom": "tsx src/example/custom-operators.ts",
    "demo:tagged": "tsx src/example/tagged-demo.ts",
    "viz": "tsx src/tools/ndjson-visualizer.ts",
    "viz:html": "tsx src/tools/ndjson-to-html.ts"
  },
  "dependencies": {
    "express": "^4.21.0",
    "rxjs": "^7.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  }
}
